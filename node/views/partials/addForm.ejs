<div class="form-popup" id="myForm">
    <form class="add-form-container" autocomplete="off">

      <div class = "icon-container">
        <div class = 'iconimg'>
        </div>
      </div>
  
      <div class="field">
        <input class="fieldinput" id="titre" maxlength="250" type="text"  name ='titre' required>
        <label class = 'fieldlabel' for="titre">Title*</label>
      </div>

      <div class="field">
        <input class="fieldinput" id="email" maxlength="250" type="text"  name ='username'  required readonly onfocus="this.removeAttribute('readonly');" >
        <label class = 'fieldlabel' for="username">Email or Username*</label>
      </div>

      <div class="field">
        <input class="fieldinput" id="hash" maxlength="250" type="password" name ='hash' value=""  required >
        <label class = 'fieldlabel' for="hash">Enter Password*</label>
      </div>

      <div class="field">
        <input class="fieldinput" id="url" maxlength="250" type="text" name ='url'  required>
        <label class = 'fieldlabel' for="url">Website url*</label>
      </div>

      <div class = 'controlbtn'>
        <input id ='submit' class="btnsubmit" type="submit" value = 'Save'></input>
        <input id ='cancel' class="btncancel" type="button" value = 'Cancel'></input>
      </div>

    </form>
  </div> 